{
    "name": "unzeroun/sorter",
    "description": "A library to dynamically sort collections / queries",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5|^3.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/property-access": "^5.4|^6.0|^7.0",
        "twig/twig": "^3.0.0",
        "vimeo/psalm": "^6.9",
        "phpunit/phpunit": "^10.5.40",
        "dg/bypass-finals": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.72",
        "symfony/twig-bundle": "^5.4|^6.0|^7.0",
        "infection/infection": "^0.28.1",
        "symfony/browser-kit": "^5.4|^6.0|^7.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/css-selector": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "UnZeroUn\\Sorter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnZeroUn\\Sorter\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "doctrine/orm": "Needed for use of ORMApplier",
        "symfony/property-access": "Needed for use of ArrayAdapter"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "yohang",
            "email": "yohan@giarel.li"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
